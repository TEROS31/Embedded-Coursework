                      00007 ;
                      00008 ; DUTY CYCLE PARAMETERS:
                      00009 ; ----------------------
                      00010 ; LED1: 20% ? 70% duty cycle
                      00011 ; LED2: 70% ? 20% duty cycle (opposite direction)
                      00012 ;
                      00013 ; TIMER0 CONFIGURATION:
                      00014 ; ---------------------
                      00015 ; Fosc = 8MHz ? Instruction cycle = 2MHz (0.5µs)
                      00016 ; Prescaler = 1:8
                      00017 ; Timer0 tick rate = 2MHz / 8 = 250kHz (4µs per tick)
                      00018 ; 
                      00019 ; PWM TIMING:
                      00020 ; -----------
                      00021 ; Target PWM period = 20ms
                      00022 ; PWM resolution = 100 steps (0-100%)
                      00023 ; Interrupt interval = 20ms / 100 = 200µs
                      00024 ; Timer0 ticks needed = 200µs / 4µs = 50 ticks
                      00025 ; Timer0 preload = 256 - 50 = 206
                      00026 ;
                      00027 ; VERIFICATION:
                      00028 ; -------------
                      00029 ; Interrupt interval = 50 × 4µs = 200µs ?
                      00030 ; PWM period = 100 × 200µs = 20ms ?
                      00031 ; 20% duty = 4ms ON, 16ms OFF
                      00032 ; 70% duty = 14ms ON, 6ms OFF
                      00033 ; Transition time = 50 PWM periods/step = 1 second per 1% change
                      00034 ;
                      00035 ; BONUS FEATURES:
                      00036 ; ---------------
                      00037 ; 4 ROM patterns using DT directives
                      00038 ; Pattern 0: 20-70% | Pattern 1: 10-40%
                      00039 ; Pattern 2: 60-90% | Pattern 3: 5-95%
                      00040 ;******************************************************************************
